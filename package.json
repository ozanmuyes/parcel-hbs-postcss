    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "helper-markdown": "^1.0.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-handlebars": "^0.4.4",
